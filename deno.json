{
    "version": "1.0.0",
    "description": "Script to select which audio tracks to keep in a video file and remove the rest.",
    "license": "GPL-3.0-only",
    "author": "",
    "main": "src/audio-fix.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/supernaut/audio-fix.git"
    },
    "bugs": {
        "url": "https://github.com/supernaut/audio-fix/issues"
    },
    "homepage": "https://github.com/supernaut/audio-fix#readme",
    "tasks": {
        "lint": "deno lint",
        "run": "deno run src/select-audio-tracks.ts",
        "compile": "deno compile --allow-env --allow-read --allow-write --allow-run --unstable --output dist/trim-audio-tracks src/main.ts",
        "compile:linux": "deno compile --allow-env --allow-read --allow-write --allow-run --unstable --output dist/linux/trim-audio-tracks --target x86_64-unknown-linux-gnu src/main.ts"
    }
}
